{
  "name": "vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:styles": "stylelint \"src/**/*.{css,scss,vue}\"",
    "test": "vitest",
    "test:e2e": "playwright test --project=smoke",
    "api:generate": "openapi-typescript ./swagger.json --output src/shared/api/generated/schema.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.90.2",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/motion": "^2.2.3",
    "axios": "^1.12.2",
    "naive-ui": "^2.39.2",
    "pinia": "^3.0.3",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.9.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "msw": "^2.11.3",
    "openapi-typescript": "^7.9.1",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "postcss-preset-env": "^10.0.4",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^7.1.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,vue}": [
      "stylelint --fix"
    ]
  }
}
